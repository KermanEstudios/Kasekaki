function teleAll(269, -59, 2729) -- Teleports everyone to the position given
    local pos = Vector3.new(269, -59, 2729) -- Finds position of the target for everyone to go to.
    for _, plr in pairs(game:GetService("Players"):GetPlayers()) do
        if plr.Character then -- If character is not a real player, then don't teleport that character.
            plr.Character:MoveTo(pos) -- Places all players in the position provided.
        end
    end
end
